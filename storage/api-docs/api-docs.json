                ]
            }
        },
        "/api/auth/refresh": {
            "post": {
                "tags": [
                    "Auth JWT"
                ],
                "summary": "Удалить акутальный токен и вернуть новый",
                "operationId": "39e2e3fac27b234a9d153240c5f7d72a",
                "requestBody": {
                    "content": {
                        "application/json": {
                            "schema": {
                                "properties": {
                                    "data": {
                                        "description": "Отправить bearer токен из header - пустой запрос",
                                        "type": "string",
                                        "example": "bearer token"
                                    }
                                },
                                "type": "object"
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "Successfully refresh new token",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "data": {
                                            "$ref": "#/components/schemas/BearerToken"
                                        },
                                        "message": {
                                            "type": "string",
                                            "example": "Successfully refresh new token"
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    },
                    "401": {
                        "description": "Не авторизован.",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "message_error": {
                                            "type": "string",
                                            "example": "Не авторизован."
                                        },
                                        "code": {
                                            "type": "integer",
                                            "example": "401"
                                        }
                                    },
                                    "type": "object"
                                }
                            }
